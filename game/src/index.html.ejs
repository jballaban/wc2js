<!DOCTYPE html>
<html>

<head>
	<link rel="shortcut icon" href="asset/favicon.gif">
	<script src="./require.js"></script>
	<script src="./game.js"></script>
	<script src="./fpsmeter.js"></script>
	<style>
		html,
		body {
			width: 100%;
			height: 100%;
			margin: 0;
			overflow: hidden;
			background-color: black;
		}

		canvas {
			position: absolute;
			left: 0px;
			top: 0px;
		}
	</style>
	<script>
		require(["Game"], function (start) {
			start.Game.init(function () {
				var diff = new Date() - new Date("<%= grunt.template.today() %>");
				return "<%= pkg.version %> compiled " + Math.floor(diff / 1000) + " seconds ago";
			}());
		});
	</script>
</head>

<body></body>

</html>